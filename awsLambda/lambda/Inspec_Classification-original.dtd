<!--************************************************************************-->
<!--                                                                        -->
<!--                   Entity Naming Conventions                            -->
<!--                   *************************                            -->
<!--                                                                        -->
<!--		   Prefix = where used:                                         -->
<!--                                                                        -->
<!--     p.   = in paragraphs (also in phrases if .ph suffix)               -->
<!--     s.   = in sections (i.e. among paragraphs)                         -->
<!--     i.   = where allowed by inclusion exceptions                       -->
<!--     m.   = content model or declared content                           -->
<!--     a.   = attribute definition                                        -->
<!--     NONE = specific use defined in models                              -->
<!--                                                                        -->
<!--	       Suffix = allowed content:                                      -->
<!--                                                                        -->
<!--     .ph  = elements whose content is %p.em.ph;                         -->
<!--     .d   = elements whose content has same model as defaults           -->
<!--     NONE = individually defined elements                               -->
<!--                                                                        -->

<!-- ********************************************************************** -->

<!-- ********************************************************************** -->
<!--                    Specialized Elements                                -->
<!--************************************************************************-->

<!ENTITY % p.em.ph   "i|b|sub|sup">                  <!-- Emphasis          -->

<!ELEMENT  i                   (#PCDATA|%p.em.ph;)*> <!-- Italic            -->
<!ELEMENT  b                   (#PCDATA|%p.em.ph;)*> <!-- Bold              -->
<!ELEMENT  sub                 (#PCDATA|%p.em.ph;)*> <!-- Inferior          -->
<!ELEMENT  sup                 (#PCDATA|%p.em.ph;)*> <!-- Superior          -->

<!--************************************************************************-->

<!--                                                                        -->
<!--                   Main Element Definitions                             -->
<!--                   ************************                             -->
<!--                                                                        -->

<!--                                                                        -->
<!--       ELEMENTS     MIN     CONTENT (EXCEPTIONS)                        -->
<!--                                                                        -->
<!ELEMENT inspclas              (contg, (rec)*)>

<!--                                                                        -->
<!--                                                                        -->
<!--                                                                        -->
<!--                             Normal record                              -->
<!--                             *************                              -->

<!ELEMENT  contg      ( file, 
                            range,
                            date)>              <!-- Control group          -->

<!ELEMENT  rec        ( cc,
                            status,
                            level,
                            title,
                            scope?,
                            sacr?,
                            scr?,
                            hisg?,
                            futg?)>             <!-- Record                 -->

<!--                                                                        -->
<!--                            Element List                                -->
<!--                                                                        -->

<!ELEMENT  cc        (#PCDATA|%p.em.ph;)*>      <!-- Classification code    -->
<!ELEMENT  ccg       (#PCDATA|%p.em.ph;|cc)*>   <!-- Classif. code group    -->

<!ELEMENT  date      (day, mo, yr)>             <!-- Date of production     -->
<!ELEMENT  day       (#PCDATA)>                 <!-- Day                    -->

<!ELEMENT  eyr       (#PCDATA)>                 <!-- End year               -->

<!ELEMENT  file      (#PCDATA)>                 <!-- File description       -->
<!ELEMENT  fut       (yr, ccg?)*>               <!-- Future scope notes     -->
<!ELEMENT  futg      (fut)*>                    <!-- Future scope notes grp.-->

<!ELEMENT  hisg      (hist)*>                   <!-- Hist. scope notes grp. -->
<!ELEMENT  hist      (yr, ccg?)*>               <!-- History scope notes    -->

<!ELEMENT  level     (#PCDATA|%p.em.ph;)*>      <!-- Level                  -->
<!ELEMENT  link      (#PCDATA|%p.em.ph;)*>      <!-- Link to CCs            -->

<!ELEMENT  mo        (#PCDATA)>                 <!-- Month                  -->

<!ELEMENT  range     (syr, eyr?)>               <!-- Year ranges for prod.  -->

<!ELEMENT  sacr      (#PCDATA|%p.em.ph;|link)*> <!-- 'See also' X-ref       -->
<!ELEMENT  scope     (#PCDATA|%p.em.ph;)*>      <!-- Scope notes            -->
<!ELEMENT  scr       (#PCDATA|%p.em.ph;|link)*> <!-- 'See' X-ref            -->
<!ELEMENT  status    (#PCDATA)>                 <!-- Status                 -->
<!ELEMENT  syr       (#PCDATA)>                 <!-- Start year             -->

<!ELEMENT  title     (#PCDATA|%p.em.ph;)*>      <!-- Classification heading -->

<!ELEMENT  yr        (#PCDATA)>                 <!-- Year                   -->

