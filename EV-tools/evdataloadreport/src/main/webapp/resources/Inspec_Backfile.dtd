<!--                         Main Element Definitions                       -->
<!--                         ************************                       -->
<!--                             from IeeCas.DTD                            -->
<!--                                                                        -->
<!--************************************************************************-->


<!ELEMENT inspecbf             (article+)                                     >

<!--************************************************************************-->
<!--                              Article                                   -->
<!--                              *******                                   -->
<!--************************************************************************-->

<!ELEMENT article              (contg, bibliog, indexg)>  <!-- Article      -->

                                   
<!--************************************************************************-->
<!--                            Control Group                               -->
<!--************************************************************************-->

<!ENTITY % m.contg       "(uid, saiss, crt?, abng+, rtyp)" > 
<!ELEMENT contg          (%m.contg;) >            <!-- Control group        -->
      <!ELEMENT uid      (#PCDATA) >              <!-- Unique ID            -->
      <!ELEMENT saiss    (#PCDATA) >              <!-- Year and issue. no.  -->
	<!ELEMENT crt      (yr, crn*) >             <!-- Copyright text       -->
		<!ELEMENT yr       (#PCDATA) >        <!-- Year                 -->
		<!ELEMENT crn      (#PCDATA) >        <!-- Copyright name       -->
      <!ELEMENT abng     (yr, vol?, iss?, abno, uidlink?)>  <!-- Abstract number group-->
            <!-- yr as in crt -->
            <!ELEMENT vol            (#PCDATA) >  <!-- Volume               -->
            <!ELEMENT iss            (#PCDATA) >  <!-- Issue month          -->
            <!ELEMENT abno           (#PCDATA) >  <!-- Abstract number      -->
            <!ATTLIST abno sec (A|B|C) "A">       <!-- Section code         -->
            <!ATTLIST abno type CDATA #IMPLIED >  <!-- "Xref" where the item has its own Abstract number but is only a cross-reference to another abstract with a different Section code within the same year -->
		<!-- uidlink as in Specialised elements -->
      <!ELEMENT rtyp                 (#PCDATA) >  <!-- Record type          -->

<!--

      The possible record types (which are not used in the DTD) are:-

      Code    Record type
      ****    ***********
       10     Dissertation
       11     Report
       12     Report Section
       21     Journal Paper
       22     Journal Paper (Original Abstracted)
       23     Journal Paper (Translation Abstracted)
       30     Book
       40     Book Chapter
       50     Conference Proceedings
       51     Conference Proceedings in Journal
       52     Conference Proceedings in Journal (Original Abstracted)
       53     Conference Proceedings in Journal (Translation Abstracted)
       60     Conference Paper
       61     Conference Paper in Journal
       62     Conference Paper in Journal (Original Abstracted)
       63     Conference Paper in Journal (Translation Abstracted)
       80     Patent
-->

<!--************************************************************************-->
<!--                            Bibliographic Group                         -->
<!--************************************************************************-->

<!ENTITY % m.bibg "(ti, aug?, jrefg?, bookg?, rptg?, dssg?, patg?, cng?, lng?, absg?, othinfo?)" >
<!ELEMENT bibliog              (%m.bibg;) >       <!-- Bibliographic group  -->
      <!ELEMENT ti             (#PCDATA|%p.em.ph;)* > <!-- Title of Article -->
      <!ELEMENT aug            (pname+) >              <!-- Author Group    -->
            <!ENTITY  % m.name "(snm, init?, pfix?, sfix?)" > <!-- Name elements   -->
            <!ELEMENT pname    (%m.name;) >              <!-- Name details  -->
                  <!ELEMENT snm  (#PCDATA|%p.em.ph;)* > <!-- Surname        -->
                  <!ELEMENT init (#PCDATA|%p.em.ph;)* > <!-- Initials       -->
                  <!ELEMENT pfix (#PCDATA|%p.em.ph;)* > <!-- Prefix         -->
                  <!ELEMENT sfix (#PCDATA|%p.em.ph;)* > <!-- Suffix         -->

<!--************************************************************************-->
<!--                       Journal Reference Group                          -->
<!--************************************************************************-->

      <!ELEMENT jrefg            (jrag*, jrtpg*) >  <!-- Journal ref. group -->

<!--************************************************************************-->
<!--                Journal Reference - Abstracted Journal                  -->
<!--************************************************************************-->

            <!ENTITY % m.jrag       "(jt?, ajt?, origajt, vid?, pgn?, pdt?, cntry?, doi?)" >
            <!ELEMENT jrag          (%m.jrag;) > <!-- Journal ref. - orig.  -->
                  <!ELEMENT jt      (#PCDATA|%p.em.ph;)* > <!-- Full Title  -->
                  <!ELEMENT ajt     (#PCDATA|%p.em.ph;)* > <!-- Modern Abbrev. Title -->
                  <!ELEMENT origajt (#PCDATA|%p.em.ph;)* > <!-- Original Abbrev. Title -->
                  <!ELEMENT vid     (vol?, ino?, other?) > <!-- Vol. + Issue -->
                        <!-- vol as in abng -->
                        <!ELEMENT ino       (#PCDATA) > <!-- Issue number   -->
                        <!ELEMENT other (#PCDATA|%p.em.ph;)* > <!-- Original vol. + Iss. -->
                  <!ELEMENT pgn     (#PCDATA) >           <!-- Pagination   -->
                  <!ELEMENT pdt (sdate?, edate?, odate?)> <!-- Publication date -->
                        <!ENTITY % m.date "(day?, mo?, yr)" > <!-- Date components -->
                        <!ELEMENT sdate (%m.date;) > <!-- Start 'sorting' date -->
                              <!ELEMENT day (#PCDATA) >   <!-- Day          -->
                              <!ELEMENT mo  (#PCDATA) >   <!-- Month        -->
                              <!-- yr as in crt -->
                        <!ELEMENT edate (%m.date;) >  <!-- End 'sorting' date -->
                        <!ELEMENT odate (#PCDATA|%p.em.ph;)* > <!-- Original date -->
                  <!ELEMENT cntry   (#PCDATA|%p.em.ph;)* > <!-- Country     -->
                  <!ELEMENT doi     (#PCDATA) > <!-- Digital Object Identifier -->

<!--************************************************************************-->
<!--              Journal Reference - Translation Partner Journal           -->
<!--************************************************************************-->
                              
            <!ENTITY % m.jrtpg  "(jt?, ajt?, origajt, vid?, pgn?, pdt?, cntry?, doi?)"  >
            <!ELEMENT jrtpg     (%m.jrtpg;) >    <!-- Journal ref. - trans. -->
                  <!-- elements as in jrag -->

<!--************************************************************************-->
<!--                            Book Group                                  -->
<!--************************************************************************-->

      <!ELEMENT bookg (pubti?, part?, editg?, section?, pgn?, pug?) > <!-- Book group -->
            <!ELEMENT pubti  (#PCDATA|%p.em.ph;)* >  <!-- Publication Title -->
            <!ELEMENT part   (#PCDATA) >             <!-- Part number       -->
            <!ELEMENT editg  (pname+) >              <!-- Editor group      -->
                  <!-- pname as in aug -->
            <!ELEMENT section (#PCDATA) >            <!-- Section           -->
            <!-- pgn as in jrag -->

<!--************************************************************************-->
<!--                            Publication Group                           -->
<!--************************************************************************-->
                              
            <!ENTITY % m.pubg            "(pdt?, pnm?, loc?, cntry?, pp?, doi?)"    >
            <!ELEMENT pug                (%m.pubg;) > <!-- Publication group -->
                  <!-- pdt as in jrag -->
                  <!ELEMENT pnm (#PCDATA|%p.em.ph;)* > <!-- Publisher's Name -->
                  <!ELEMENT loc (#PCDATA|%p.em.ph;)* > <!-- Place of Publication (or Location of Conference) -->
                  <!-- cntry as in jrag -->
                  <!ELEMENT pp  (#PCDATA) >            <!-- Page nos.       -->
                  <!-- doi as in jrag -->

<!--************************************************************************-->
<!--                                 Report Group                           -->
<!--************************************************************************-->
                              
      <!ENTITY % m.repg "(pubti?, editg?, issorg, repno?, pgn?, pug?)"        >
      <!ELEMENT rptg                   (%m.repg;) >     <!-- Report group   -->
            <!-- pubti as in bookg -->
            <!-- editg as in bookg -->
            <!ELEMENT issorg     (orgn, cntry?) > <!-- Issuing organisation -->
                  <!ELEMENT orgn (#PCDATA|%p.em.ph;)* > <!-- Organisation name -->
                  <!-- cntry as in jrag -->
            <!ELEMENT repno      (#PCDATA) >            <!-- Report number  -->
            <!-- pgn as in jrag -->
            <!-- pug as in bookg -->

<!--************************************************************************-->
<!--                            Dissertation Group                          -->
<!--************************************************************************-->
                              
      <!ENTITY % m.dissg        "(subdt?, issorg, pug?)"                      >
      <!ELEMENT dssg            (%m.dissg;) >       <!-- Dissertation group -->
            <!ELEMENT subdt     (sdate?, odate?) >  <!-- Date submitted     -->
                  <!-- sdate as in pdt -->
                  <!-- odate as in pdt -->
            <!-- issorg as in rptg -->
            <!-- pug as in bookg -->

<!--************************************************************************-->
<!--                               Patent Group                             -->
<!--************************************************************************-->

      <!ENTITY % m.patg               "(pdg?, pdt?, cntry?, pp?)"             >
      <!ELEMENT  patg                 (%m.patg;) >      <!-- Patent group   -->
            <!ELEMENT  pdg            (patno?, cntry?, subdt?, assg?)> <!--Patent details grp.-->
                  <!ELEMENT  patno    (#PCDATA)     >   <!--Patent number   -->
                  <!-- cntry as in jrag (Country of patent) -->
                  <!-- subdt as in dssg -->
                  <!ELEMENT  assg     (assignee+) > <!--Patent assignee grp.-->
                        <!ELEMENT  assignee (#PCDATA|%p.em.ph;)*> <!--Patent assignee-->
            <!-- pdt as in jrag -->
            <!-- cntry as in jrag (Country of publication) -->
            <!-- pp as in pug -->

<!--************************************************************************-->
<!--                           Conference Group                             -->
<!--************************************************************************-->
                              
      <!ENTITY % m.cng     "(ct?, cndt?, loc?, cntry?, cnsg?)"                >
      <!ELEMENT cng        (%m.cng;) >               <!-- Conference group  -->
            <!ELEMENT ct   (#PCDATA|%p.em.ph;)* >    <!-- Conference title  -->
            <!ELEMENT cndt (sdate?, edate?, odate?) > <!-- Conference dates -->
                  <!-- sdate as in pdt -->
                  <!-- edate as in pdt -->
                  <!-- odate as in pdt -->
            <!-- loc as in pug -->
            <!-- cntry as in jrag -->
            <!ELEMENT cnsg (orgn+) >             <!-- Conf. sponsors group  -->
                  <!-- orgn as in rptg -->

<!--************************************************************************-->
<!--                            Language Group                              -->
<!--************************************************************************-->

      <!ELEMENT lng       (lan+) >                <!-- Language group       -->
            <!ELEMENT lan (#PCDATA|%p.em.ph;)* >  <!-- Language             -->

<!--************************************************************************-->
<!--                            Abstract Group                              -->
<!--************************************************************************-->

      <!ELEMENT       absg (abs?, fig*) >          <!-- Abstract group      -->
            <!ELEMENT abs %m.ph.table; >           <!-- Abstract            -->
            <!ELEMENT fig (image, caption?) >      <!-- Figure              -->
                  <!ELEMENT image   (#PCDATA) >    <!-- Image               -->
                  <!ATTLIST image img CDATA #REQUIRED > <!-- Image name     -->
                  <!ELEMENT caption (#PCDATA|%p.em.ph;)* > <!-- Caption to image -->

<!--************************************************************************-->
<!--                            Other Information                           -->
<!--************************************************************************-->

      <!ELEMENT othinfo (#PCDATA|%p.em.ph.u;)* > <!-- Other Information -->

<!--************************************************************************-->
<!--                            Indexing Group                              -->
<!--************************************************************************-->
                              
<!ENTITY % m.indg            "(cindg, origind?, ucindg?, ccg, origcc?, udcg?)" >
<!ELEMENT indexg             (%m.indg;) >           <!-- Index group        -->
	<!ELEMENT cindg        (term+) > <!-- Modern Controlled Indexing Group -->
		<!ELEMENT term   (#PCDATA|%p.em.ph;)* > <!-- Subject Indexing Term -->

	<!ELEMENT origind     (termg+) >          <!-- Original Controlled Indexing -->
		<!ELEMENT termg (mterm?, sterm?, ssterm?, mod) >      <!-- Term Group -->
			<!ELEMENT mterm  (#PCDATA|%p.em.ph;)* > <!-- Main Indexing Term -->
			<!ELEMENT sterm  (#PCDATA|%p.em.ph;)* > <!-- Sub-Indexing Term -->
			<!ELEMENT ssterm (#PCDATA|%p.em.ph;)* > <!-- Sub-Sub-Indexing Term -->
			<!ELEMENT mod    (#PCDATA|%p.em.ph;)* > <!-- Modifier -->

	<!ELEMENT ucindg       (term*) > <!-- Uncontrolled Indexing Group -->
	     	<!-- term as in cindg -->

	<!ELEMENT ccg          (cc+) > <!-- Modern Classification Group -->
		<!ELEMENT cc     (code?, cct) > <!-- Modern Classification Term -->
		<!ATTLIST cc type CDATA #IMPLIED > <!-- "prime" for First-Place Classification -->
			<!ELEMENT code   (#PCDATA) >    <!-- Classification Code -->
			<!ELEMENT cct (#PCDATA|%p.em.ph;)* > <!-- Classification Text -->

	<!ELEMENT origcc       (cc+) > <!-- Original Classification -->
		<!-- cc as in ccg -->
			<!ATTLIST cct cmh  CDATA #IMPLIED > <!-- Classification Main Heading -->

	<!ELEMENT udcg (udc)* >                                <!-- UDC Group -->
		<!ELEMENT udc (#PCDATA) > <!-- Universal Decimal Classification -->
