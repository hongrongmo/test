<POOLS>
<CONNECTION NAME="village_log"
	    URL="{{JDBC_LOG_URL}}"
	    USERNAME="{{JDBC_LOG_USERNAME}}"
	    PASSWORD="{{JDBC_LOG_PASSWORD}}"
	    DRIVER="{{JDBC_LOG_DRIVER}}"
	    MINCONNECTIONS="{{JDBC_LOG_MINCON}}"
	    MAXCONNECTIONS="{{JDBC_LOG_MAXCON}}"
	    REFRESHINTERVAL="1.0"
	    LOGFILE="{{LOGS_DIR}}logservice_pool.log"></CONNECTION>


</POOLS>

