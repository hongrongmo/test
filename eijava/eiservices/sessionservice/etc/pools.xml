<POOLS>

<CONNECTION NAME="AuthPool"
	    URL="{{JDBC_AUTH_URL}}"
	    USERNAME="{{JDBC_AUTH_USERNAME}}"
	    PASSWORD="{{JDBC_AUTH_PASSWORD}}"
	    DRIVER="{{JDBC_AUTH_DRIVER}}"
	    MINCONNECTIONS="{{JDBC_AUTH_MINCON}}"
	    MAXCONNECTIONS="{{JDBC_AUTH_MAXCON}}"
	    REFRESHINTERVAL="1.0"
	    LOGFILE="{{LOGS_DIR}}userservice_pool.log"></CONNECTION>
	    
<CONNECTION NAME="SessionPool"
	    URL="{{JDBC_SES_URL}}"
	    USERNAME="{{JDBC_SES_USERNAME}}"
	    PASSWORD="{{JDBC_SES_PASSWORD}}"
	    DRIVER="{{JDBC_SES_DRIVER}}"
	    MINCONNECTIONS="{{JDBC_SES_MINCON}}"
	    MAXCONNECTIONS="{{JDBC_SES_MAXCON}}"
	    REFRESHINTERVAL="1.0"
	    LOGFILE="{{LOGS_DIR}}sessionservice_pool.log"></CONNECTION>

</POOLS>


