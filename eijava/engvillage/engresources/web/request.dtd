<!ELEMENT REQUEST (	FORMAT,
					DATABASE,
					QUERYWRAPPER?,
					SORT?,
					STARTYEAR?,
					ENDYEAR?,
					YEARSELECT?,
					PAGE?,
					SEARCHID?,
					EISESSION?)>
<!ELEMENT FORMAT 		(#PCDATA)>					
