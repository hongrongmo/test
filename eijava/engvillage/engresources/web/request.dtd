<!ELEMENT STARTYEAR 	(#PCDATA)>
<!ELEMENT ENDYEAR 		(#PCDATA)>
<!ELEMENT YEARSELECT 	(#PCDATA)>
<!ELEMENT PAGE 			(#PCDATA)>
<!ELEMENT SEARCHID 		(#PCDATA)>
<!ELEMENT EISESSION		(#PCDATA)>
<!ELEMENT QUERY			(word*,andQuery*,orQuery*,notQuery*)>
