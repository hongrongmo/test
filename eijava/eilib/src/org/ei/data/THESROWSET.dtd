<!ELEMENT ROWSET (ROW+)>
<!ELEMENT ROW (TDOCID,
               DATABASE,
               SORTNUMBER,
               MAINTERM,
               BROADERTERMS,
               NARROWERTERMS,
               RELATEDTERMS,
               TOPTERMS,
               PRIORTERMS
               )>

<!ELEMENT TDOCID (#PCDATA) >
<!--
	CPX Map: DATABASE + "_" + T_ID
	INSPEC Map: DATABASE + "_" + T_ID
-->


<!ELEMENT DATABASE (#PCDATA) >
<!--
	CPX Map: AUTO Generated
	INSPEC Map: AUTO Generated
-->


<!ELEMENT SORTNUMBER (#PCDATA) >
<!--
	CPX Map:T_ID
	INSPEC Map: T_ID
-->

<!ELEMENT MAINTERM (#PCDATA) >
<!--
	CPX Map: MAIN_TERM_SEARCH
	INSPEC Map: MAIN_TERM_SEARCH
-->


<!ELEMENT BROADERTERMS (#PCDATA) >
<!--
	CPX Map: BROADER_TERMS
	INSPEC Map: BROADER_TERMS
	Formating Note:  This field is a semi-colon delimited list.
	
-->

<!ELEMENT NARROWERTERMS (#PCDATA) >
<!--
